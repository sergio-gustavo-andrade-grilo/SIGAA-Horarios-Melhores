{
  "manifest_version": 3,
  "name": "SIGAA UFCG: Horários Melhores",
  "description": "Converte os horários do SIGAA da UFCG para um formato mais compreensível.",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://sigaa.ufcg.edu.br/*"]
    }
  ]
}
